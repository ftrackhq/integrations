{
  "type": "publisher",
  "name": "Image Sequence Publisher",
  "asset_type": "img",
  "host_type": "harmony",
  "ui_type": "qt",
  "contexts": [
    {
      "name": "main",
      "stages": [
        {
          "name": "context",
          "plugins":[
            {
              "name": "context selector",
              "plugin": "common_passthrough_publisher_context",
              "widget": "common_default_publisher_context"
            }
          ]
        }
      ]
    }
  ],
  "components": [
    {
      "name": "sequence",
      "optional": false,
      "stages": [
        {
          "name": "collector",
          "plugins":[
            {
             "name": "empty collector",
              "plugin": "common_test_publisher_collector"
            }
          ]
        },
        {
          "name": "validator",
          "plugins":[
            {
              "name": "validate selection",
              "plugin": "common_test_publisher_validator"
            }
          ]
        },
        {
          "name": "exporter",
          "plugins":[
            {
              "name": "render sequence",
              "plugin": "harmony_sequence_publisher_exporter"
            }
          ]
        }
      ]
    },
    {
      "name": "reviewable",
      "optional": true,
      "enabled": false,
      "stages": [
        {
          "name": "collector",
          "plugins":[
            {
             "name": "empty collector",
              "plugin": "common_test_publisher_collector"
            }
          ]
        },
        {
          "name": "validator",
          "plugins":[
            {
              "name": "validate selection",
              "plugin": "common_test_publisher_validator"
            }
          ]
        },
        {
          "name": "exporter",
          "plugins":[
            {
              "name": "transcode reviewable from sequence",
              "plugin": "harmony_reviewable_publisher_exporter"
            }
          ]
        }
      ]
    },
    {
      "name": "thumbnail",
      "optional": true,
      "enabled": false,
      "stages": [
        {
          "name": "collector",
          "plugins":[
            {
              "name": "Viewport",
              "plugin": "common_passthrough_publisher_collector"
            }
          ]
        },
        {
          "name": "validator",
          "plugins":[
            {
              "name": "Unused validator",
              "plugin": "common_passthrough_publisher_validator"
            }
          ]
        },
        {
          "name": "exporter",
          "plugins":[
            {
              "name": "Grab screen shot from node graph",
              "plugin": "harmony_thumbnail_publisher_exporter"
            }
          ]
        }
      ]
    }
  ],
  "finalizers": [
    {
      "name": "main",
      "stages": [
        {
          "name": "pre_finalizer",
          "visible": false,
          "plugins":[
            {
              "name": "Pre publish to ftrack server",
              "plugin": "common_passthrough_publisher_pre_finalizer"
            }
          ]
        },
        {
          "name": "finalizer",
          "visible": false,
          "plugins":[
            {
              "name": "Publish to ftrack server",
              "plugin": "common_passthrough_publisher_finalizer"
            }
          ]
        },
        {
          "name": "post_finalizer",
          "visible": false,
          "plugins":[
            {
              "name": "Post process publish",
              "plugin": "common_passthrough_publisher_post_finalizer"
            }
          ]
        }
      ]
    }
  ]
}